  priority: 50,
  defining: true,
  parseHTML() {
    return [
      {
        tag: "div",
        getAttrs: (e) => {
          if (typeof e == "string")
            return false;
          const t = {};
          for (const [o, i] of Object.entries(ra))
            e.getAttribute(i) && (t[o] = e.getAttribute(i));
          return e.getAttribute("data-node-type") === "blockContainer" ? t : false;
        }
      }
    ];
  },
