          n = true;
          const { start: f2, ...g } = u.attrs;
          i.setNodeMarkup(d.blockContent.beforePos, void 0, {
            ...g,
            index: l,
            ...typeof f2 == "number" && m && {
              start: f2
            }
          });
        }
      }
    }), n ? i : null;
  }
