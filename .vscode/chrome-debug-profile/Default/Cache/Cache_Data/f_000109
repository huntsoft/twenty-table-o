  let isNode2 = slice.openStart == 0 && slice.openEnd == 0 && slice.content.childCount == 1;
  let beforeInsert = tr4.doc;
  if (isNode2)
    tr4.replaceRangeWith(pos, pos, slice.content.firstChild);
  else
    tr4.replaceRange(pos, pos, slice);
