    return;
  event.preventDefault();
  let insertPos = slice ? dropPoint2(view.state.doc, $mouse.pos, slice) : $mouse.pos;
  if (insertPos == null)
    insertPos = $mouse.pos;
  let tr4 = view.state.tr;
  if (move) {
    let { node } = dragging;
    if (node)
      node.replace(tr4);
    else
      tr4.deleteSelection();
  }
