                  return f2.className = "bn-table-drop-cursor", f2.style.left = "0", f2.style.right = "0", n > this.view.state.draggingState.originalIndex ? f2.style.bottom = "-2px" : f2.style.top = "-3px", f2.style.height = "4px", f2;
