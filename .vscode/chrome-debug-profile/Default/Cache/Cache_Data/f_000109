              m
            );
          }
        }
        return {
          destroy: () => {
            var c, u;
            ((c = t.prosemirrorView) == null ? void 0 : c.root) instanceof ShadowRoot ? t.prosemirrorView.root.removeChild(i) : (u = t.prosemirrorView) == null || u.root.head.removeChild(i);
          }
        };
      },
      props: {
