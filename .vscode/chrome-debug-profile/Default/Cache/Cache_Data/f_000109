var ne = class {
  constructor() {
    p2(this, "callbacks", {});
  }
  on(t, o) {
    return this.callbacks[t] || (this.callbacks[t] = []), this.callbacks[t].push(o), () => this.off(t, o);
  }
  emit(t, ...o) {
    const i = this.callbacks[t];
    i && i.forEach((n) => n.apply(this, o));
  }
  off(t, o) {
    const i = this.callbacks[t];
    i && (o ? this.callbacks[t] = i.filter((n) => n !== o) : delete this.callbacks[t]);
  }
  removeAllListeners() {
    this.callbacks = {};
  }
};
var xr = class {
  constructor(t, o, i, n) {
