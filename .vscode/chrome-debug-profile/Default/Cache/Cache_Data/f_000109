          t._tiptapEditor.state.doc,
          g.pos
        ), k = F(b);
        u = t.insertBlocks(
          [c],
          k.bnBlock.node.attrs.id,
          "after"
        )[0].id;
      } else
        return;
      const h = await t.uploadFile(d, u), m = typeof h == "string" ? {
        props: {
          url: h
        }
      } : { ...h };
      t.updateBlock(u, m);
    }
  }
}
