            renderHTML: (e) => e.textColor === x.textColor.default ? {} : {
              "data-text-color": e.textColor
            }
          }
        }
      }
    ];
  }
});
var na = Extension.create({
  name: "trailingNode",
  addProseMirrorPlugins() {
