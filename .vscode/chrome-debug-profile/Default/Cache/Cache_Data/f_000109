    e,
    r4.posBeforeNode,
    o
  )({ state: s, dispatch: l }), true));
  const a = i.state.doc.resolve(r4.posBeforeNode + 1).node();
  return w(
    a,
    e.schema.blockSchema,
    e.schema.inlineContentSchema,
    e.schema.styleSchema,
    e.blockCache
  );
}
