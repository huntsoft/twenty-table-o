  let slice = dragging && dragging.slice;
  if (slice) {
    view.someProp("transformPasted", (f2) => {
      slice = f2(slice, view);
    });
  } else {
    slice = parseFromClipboard2(view, getText2(event.dataTransfer), brokenClipboardAPI2 ? null : event.dataTransfer.getData("text/html"), false, $mouse);
  }
